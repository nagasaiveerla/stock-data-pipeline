# Stock Data Pipeline Environment Variables
# Copy this file to .env and update with your actual values

# Alpha Vantage API Configuration
# Get your free API key from: https://www.alphavantage.co/support/#api-key
ALPHA_VANTAGE_API_KEY=your_actual_api_key_here

# Stock Symbols to Process (comma-separated)
STOCK_SYMBOLS=AAPL,GOOGL,MSFT,TSLA,AMZN,NVDA,META,NFLX

# Pipeline Processing Configuration
BATCH_SIZE=5
MAX_WORKERS=3

# API Request Configuration
API_TIMEOUT=30
API_RETRY_ATTEMPTS=3
API_RETRY_DELAY=5

# Database Configuration (usually handled by Docker Compose)
POSTGRES_HOST=stock-postgres
POSTGRES_DB=stockdata
POSTGRES_USER=stockuser
POSTGRES_PASSWORD=stockpass

# Airflow Configuration
AIRFLOW_UID=50000
AIRFLOW_PROJ_DIR=.
_AIRFLOW_WWW_USER_USERNAME=airflow
_AIRFLOW_WWW_USER_PASSWORD=airflow

# Logging Configuration
LOG_LEVEL=INFO
